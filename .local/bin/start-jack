#!/bin/sh

# According
# https://wiki.archlinux.org/title/JACK_Audio_Connection_Kit

jack_control stop
jack_control ds alsa
jack_control dps device hw:0
jack_control dps rate 48000
jack_control dps nperiods 2
jack_control dps period 1024
jack_control start
sleep 5
a2j_control --ehw
a2j_control --start

